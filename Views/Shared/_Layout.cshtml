    @{
        string ID = string.Empty;
        string Name = string.Empty;
        string VIPUserRole = string.Empty;
        string CountryList = string.Empty;
        string obj = string.Empty;
        if (User.Identity.IsAuthenticated)
        {
            obj = User.Identity.Name;
            ID = obj.Split('_')[0];
            Name = obj.Split('_')[1];
            VIPUserRole = obj.Split('_')[2];
            CountryList = obj.Split('_')[3];
        }
        else
        {
            Response.Redirect(Url.Content("~/Home/Login") + "?returnUrl=" + Request.RawUrl);
        }
    }

