
@{
    ViewBag.Title = "PartnerLoginFrequent";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="page-wrapper">
    <!-- Page body start -->
    <div class="page-body">
        <div class="row">
            <div class="col-sm-12">
                <!-- Basic Form Inputs card start -->
                <div class="card">
                    <div class="card-header">
                        <h5>Partner Log in Frequency</h5>
                    </div>
                    <div class="card-block">
                        <div class="table-responsive">
                            <table id="tblPartnerLoginLog" class="table table-striped table-bordered" style="width:100%">
                                <thead>
                                    <tr>
                                        <th>Action</th>
                                        <th>Country</th>
                                        <th>Company</th>
                                        <th>Name</th>
                                        <th>Email</th>
                                        <th>Frequent</th>
                                    </tr>
                                </thead>
                            </table>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
    <!-- Page body end -->
</div>

<div id="PartnerLoginHistoryModal">
    <div class="md-content">
        <div class="table-responsive">
            <table id="tblPartnerLoginLogDetail" class="table table-striped" style="width:100%">
                <thead>
                    <tr>
                        <th>Email</th>
                        <th>Log in Date</th>
                    </tr>
                </thead>
            </table>
        </div>
        <div class="form-group row div-centerbtn">
            <div class="col-sm-3">
                <button type="button" id="btnOK" onclick="PartnerLoginHistoryClose();" class="btn btn-block btn-grd-inverse">Close</button>
            </div>
        </div>
    </div>
</div>

<input id="HGetPartnerLoginLog" type="hidden" value="@Url.Action("GetPartnerLoginLog", "api/PartnerApi")" />
<input id="HGetPartnerLoginLogDetail" type="hidden" value="@Url.Action("GetPartnerLoginLogDetail", "api/PartnerApi")" />

@section Scripts{
    <script src="@Url.Content("~/Scripts/partner.js?v=1")"></script>

    <script type="text/javascript">
        $(document).ready(function () {
            PartnerloginLogLoad();
        });
    </script>
}


